{
  "name": "yogarine/revision-pdo-laravel",
  "description": "RevisionPDO integration for Laravel",
  "type": "library",
  "homepage": "https://bitbucket.org/Yogarine/revisionpdo-laravel",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Alwin Garside",
      "email": "alwin@garsi.de"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "^4.1 || ^5.0",
    "yogarine/revision-pdo": "~0.1.0"
  },
  "autoload" : {
    "psr-0" : {
      "RevisionPDO\\" : "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "RevisionPDO\\Illuminate\\Database\\RevisionPDOServiceProvider"
      ]
    }
  }
}
